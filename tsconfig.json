{
  "compilerOptions": {
    "baseUrl": "docs",
    "rootDir": "docs",
    "module": "ES2015",
    "moduleResolution": "node",
    "declaration": true,
    "outDir": "docs/.vuepress/dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "target": "ES2015",
    "strict": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitOverride": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "paths": {
      "@/docs/*": ["docs/.vuepress/*"]
    },
    "lib": ["es2020", "dom"]
  },
  "exclude": ["node_modules", "docs/.cache/*", "docs/.temp/*", "docs/dist/*"],
  "include": ["docs/.vuepress/*"]
}
